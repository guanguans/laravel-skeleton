{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "alexandre-daubois/monolog-processor-collection": "^1.3",
        "anlutro/l4-settings": "^1.4",
