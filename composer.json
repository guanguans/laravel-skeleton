{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "awobaz/compoships": "^2.1",
        "bensampo/laravel-enum": "^4.0",
        "beyondcode/laravel-query-detector": "^1.5",
        "biiiiiigmonster/hasin": "^1.0",
        "binarytorch/larecipe": "^2.5",
        "brainmaestro/composer-git-hooks": "^2.8",
        "dyrynda/laravel-cascade-soft-deletes": "^4.1",
        "f9webltd/laravel-api-response-helpers": "^1.5",
        "fideloper/proxy": "^4.4",
        "flugger/laravel-responder": "^3.2",
        "fruitcake/laravel-cors": "^2.0",
        "goldspecdigital/laravel-eloquent-uuid": "^8.0",
        "guanguans/laravel-dump-sql": "^2.2",
        "guanguans/laravel-exception-notify": "^2.11",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/agent": "^2.6",
        "jiannei/laravel-response": "^4.4",
        "kitloong/laravel-migrations-generator": "^5.1",
        "knuckleswtf/scribe": "^2.7",
        "laravel/framework": "^8.54",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "orangehill/iseed": "^3.0",
        "overtrue/laravel-lang": "^5.0",
        "overtrue/laravel-query-logger": "^2.1",
        "propaganistas/laravel-disposable-email": "^2.1",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "rennokki/laravel-eloquent-query-cache": "^3.2",
        "runtime/laravel": "^0.1.1",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-http-logger": "^1.8",
        "spatie/laravel-json-api-paginate": "^1.10",
        "spatie/laravel-missing-page-redirector": "^2.9",
        "spatie/laravel-translatable": "^4.6",
        "staudenmeir/belongs-to-through": "^2.11",
        "staudenmeir/eloquent-has-many-deep": "^1.14",
        "staudenmeir/eloquent-json-relations": "^1.5",
        "symfony/stopwatch": "^5.3",
        "tenantcloud/laravel-boolean-softdeletes": "^3.1",
        "timacdonald/has-parameters": "^1.2",
        "tymon/jwt-auth": "^1.0",
        "vectorface/whip": "^0.4.0",
        "watson/validating": "^6.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "davidhsianturi/laravel-compass": "^1.2",
        "deployer/deployer": "^6.8",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "lanin/laravel-api-debugger": "^4.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0",
        "phpunit/phpunit": "^9.3.3",
        "reliese/laravel": "^1.1",
        "stechstudio/laravel-php-cs-fixer": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "hooks": {
            "pre-commit": [
                "composer checks"
            ],
            "post-merge": [
                "composer checks"
            ]
        },
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper",
                "lanin/laravel-api-debugger",
                "nunomaduro/collision"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@cghooks add --ignore-lock",
            "@cghooks update"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@cghooks update"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "checks": [
            "@mark-start",
            "@style-lint",
            "@mark-separate",
            "@docs-generate",
            "@mark-finish"
        ],
        "cghooks": "./vendor/bin/cghooks",
        "docs-generate": "@php artisan scribe:generate --ansi",
        "mark-finish": "printf '\\n!\\n!\\t\\033[0;32m%s\\033[0m\\n!\\n\\n' \"Finished\"",
        "mark-separate": "printf '\\n!\\n!\\t\\033[0;33m%s\\033[0m\\n!\\n\\n' \"----------------\"",
        "mark-start": "printf '\\n!\\n!\\t\\033[0;36m%s\\033[0m\\n!\\n\\n' \"Started\"",
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=1G --ansi -vv",
        "phpstan-baseline": "@phpstan --generate-baseline=phpstan-baseline.neon",
        "style-fix": "@php artisan fixer:fix --ansi",
        "style-lint": "@php artisan fixer:fix --diff --dry-run --ansi"
    }
}
