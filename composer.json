    "repositories": {
        "facade-documenter": {
            "type": "vcs",
            "url": "https://github.com/laravel/facade-documenter.git"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "captainhook/hook-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true,
            "wikimedia/composer-merge-plugin": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": true,
            "target-directory": "vendor-bin"
        },
        "hooks": {
            "post-merge": [
                "composer checks"
            ],
            "pre-commit": [
                "composer checks"
            ]
        },
        "laravel": {
            "dont-discover": [
                "andreaselia/laravel-api-to-postman",
                "barryvdh/laravel-ide-helper",
                "bennett-treptow/laravel-migration-generator",
                "chrisdicarlo/laravel-config-checker",
                "dragon-code/pretty-routes",
                "enlightn/enlightn",
                "guanguans/laravel-soar",
                "kitloong/laravel-migrations-generator",
                "ladumor/laravel-pwa",
                "lanin/laravel-api-debugger",
                "laracademy/generators",
                "laracraft-tech/laravel-schema-rules",
                "laradumps/laradumps",
                "laravel-shift/factory-generator",
                "laravel/sail",
                "laravel/telescope",
                "mortexa/laravel-arkitect",
                "msamgan/laravel-env-keys-checker",
                "muhammadhuzaifa/telescope-guzzle-watcher",
                "nunomaduro/collision",
                "orangehill/iseed",
                "reliese/laravel",
                "salahhusa9/laravel-updater",
                "scalar/laravel",
                "soloterm/solo",
                "spatie/laravel-error-solutions",
                "spatie/laravel-ignition",
                "spatie/laravel-stubs",
                "thedoctor0/laravel-factory-generator",
                "worksome/envy"
            ]
        },
        "merge-plugin": {
            "ignore-duplicates": false,
            "include": [
                "composer.local.json",
                "modules/*/composer.json"
            ],
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-replace": true,
            "merge-scripts": false,
            "recurse": true,
            "replace": false,
            "require": []
        }
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --force --ansi -v",
            "@php artisan spatie-stub:publish --force --ansi -v",
            "@php artisan ide-helper:generate --ansi -v",
            "@php artisan ide-helper:meta --ansi -v"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi -v"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi -v"
        ],
        "ai-commit": "@php ./vendor/bin/ai-commit commit --ansi",
        "ai-commit-bito": "@ai-commit --generator=bito_cli",
        "ai-commit-bito-no-verify": "@ai-commit-bito --no-verify",
        "ai-commit-github-copilot": "@ai-commit --generator=github_copilot_cli",
        "ai-commit-github-copilot-no-verify": "@ai-commit-github-copilot --no-verify",
        "ai-commit-github-models": "@ai-commit --generator=github_models_cli",
        "ai-commit-github-models-no-verify": "@ai-commit-github-models --no-verify",
        "ai-commit-no-verify": "@ai-commit --no-verify",
        "arkitect": "@php artisan test:arkitect --ansi -v",
        "artisan": [
            "@putenv XDEBUG_MODE=coverage,debug",
            "@putenv XDEBUG_SESSION=1",
            "@php artisan --ansi"
        ],
        "captainhook": "@php ./vendor/bin/captainhook --ansi -v",
        "captainhook-configure": "@captainhook configure",
        "captainhook-install": "@captainhook install",
        "cghooks": "@php ./vendor/bin/cghooks --ansi -v",
        "cghooks-ignore": "[ ! -f \"./vendor/bin/cghooks\" ] && exit 0 || php ./vendor/bin/cghooks --ansi -v",
        "checks": [
            "@skeletor",
            "@composer-validate",
            "@sk-check-conflicts",
            "@pint-dirty-test",
            "@phpstan",
            "@rector-dry-run"
        ],
        "composer-audit": "@composer audit --ansi -v",
        "composer-bin-all-update": "@composer bin all update --ansi -v",
        "composer-check-platform-reqs": "@composer check-platform-reqs --ansi -v",
        "composer-diff": "@composer diff --with-platform --ansi -v",
        "composer-normalize": "@composer normalize --dry-run --diff --ansi -v",
        "composer-parallel": "@composer parallel --ansi -v",
        "composer-parallel-checks": "@composer-parallel composer-validate md-lint lint style-lint test psalm",
        "composer-require-checker": "@php ./vendor/bin/composer-require-checker check --config-file=composer-require-checker.json composer.json --ansi -v",
        "composer-unused": "@php ./vendor/bin/composer-unused --ansi -v",
        "composer-unused-checker": "@php ./vendor/bin/composer-unused --ansi -v",
        "composer-updater": "@php ./composer-updater --highest-php-binary=/opt/homebrew/opt/php@8.4/bin/php --ansi",
        "composer-updater-dry-run": "@composer-updater --dry-run",
        "composer-updater-laravel10": "@composer-updater --composer-json-path=vendor-bin/laravel10/composer.json",
        "composer-validate": "@composer validate --check-lock --strict --ansi -v",
        "docs-generate": "@php artisan scribe:generate --ansi -v",
        "envoy": "@php ./vendor/bin/envoy --ansi -v",
        "envoy-local": "@envoy run local",
        "envoy-ssh-testing": "@envoy ssh testing",
        "envoy-testing": "@envoy run testing",
        "envy-prune": "@php artisan envy:prune --ansi -v",
        "envy-sync": "@php artisan envy:sync --ansi -v",
        "facade-lint": "@facade-update --lint",
        "facade-update": "@php -f ./facade.php -- $(php -r 'echo implode('\\'' '\\'', array_map(fn ($f) => str_replace(['\\''app'\\'', '\\''/'\\'', '\\''.php'\\''], ['\\''App'\\'', '\\''\\\\'\\'', '\\'''\\''], $f), glob('\\''app/Support/Facades/*.php'\\'')));')",
        "hsamac": "./vendor/bin/hsamac",
        "hsamac-check": "@hsamac check",
        "hsamac-replace": "@hsamac replace",
        "ide-helper": [
            "@php artisan ide-helper:eloquent --ansi -v",
            "@php artisan ide-helper:generate --ansi -v",
            "@php artisan ide-helper:meta --ansi -v",
            "@php artisan ide-helper:models --nowrite --ansi -v",
            "@php artisan admin:ide-helper --ansi -v"
        ],
        "lint": [
            "for DIR in .; do find $DIR -maxdepth 1 -type f -name '*.php' -type f ! -name 'xxx.php' -exec /opt/homebrew/opt/php@8.1/bin/php -l {} \\; 2>&1 | (! grep -v '^No syntax errors detected'); done",
            "for DIR in ./app ./config ./database ./lang ./resources ./routes ./tests; do find $DIR -type f -name '*.php' -type f ! -name 'xxx.php' -exec /opt/homebrew/opt/php@8.1/bin/php -l {} \\; 2>&1 | (! grep -v '^No syntax errors detected'); done"
        ],
        "md-fix": "@md-lint --fix",
        "md-lint": "lint-md --config .lintmdrc ./*.md ./.github/",
        "pest": "@php ./vendor/bin/pest --coverage",
        "pest-coverage": "@pest --coverage-html=./.build/phpunit/ --coverage-clover=./clover.xml --coverage",
        "pest-migrate-configuration": "@pest --migrate-configuration",
        "phpstan": "@php ./vendor/bin/phpstan analyse --memory-limit=1G --ansi -v",
        "phpstan-baseline": "@phpstan --generate-baseline=phpstan-baseline.neon",
        "pint": "@php ./vendor/bin/pint --ansi -v",
        "pint-dirty": "@pint --dirty",
        "pint-dirty-test": "@pint-dirty --test --bail",
        "pint-test": "@pint --test",
        "rector": "@php ./vendor/bin/rector --ansi -v",
        "rector-clear-cache": "@rector --clear-cache",
        "rector-clear-cache-dry-run": "@rector-clear-cache --dry-run",
        "rector-custom-rule": "@rector custom-rule",
        "rector-detect-node": "@rector detect-node --loop",
        "rector-dry-run": "@rector --dry-run",
        "rector-list-rules": "@rector list-rules",
        "rector-setup-ci": "@rector setup-ci",
        "sk": "@php ./vendor/bin/swiss-knife --ansi -v",
        "sk-check-commented-code": "@sk check-commented-code ./app ./routes --line-limit=5",
        "sk-check-conflicts": "@sk check-conflicts ./app ./config ./database ./resources ./routes ./tests",
        "sk-finalize-classes": "@sk finalize-classes ./app",
        "sk-finalize-classes-dry-run": "@sk-finalize-classes --dry-run",
        "sk-find-multi-classes": "@sk find-multi-classes ./app ./routes",
        "sk-namespace-to-psr-4": "@sk namespace-to-psr-4 ./app --namespace-root=App\\",
        "skeletor": [
            "@putenv NO_SKELETOR_CLEANUP=true",
            "NiftyCo\\Skeletor\\Runner::execute"
        ],
        "style-fix": "@php ./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --show-progress=dots --diff --ansi -v",
        "style-fix-dist": "@php ./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer-dist.php --show-progress=dots --diff --ansi -v",
        "style-lint": "@style-fix --dry-run",
        "style-lint-dist": "@style-fix-dist --dry-run",
