    "repositories": {
        "facade-documenter": {
            "type": "vcs",
            "url": "https://github.com/laravel/facade-documenter.git"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php",
            "vendor/attributes.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "carthage-software/mago": true,
            "cweagans/composer-patches": true,
            "ergebnis/composer-normalize": true,
            "ion-bazan/composer-diff": true,
            "olvlvl/composer-attribute-collector": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true,
            "symfony/thanks": true,
            "wikimedia/composer-merge-plugin": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": false,
            "target-directory": "vendor-bin"
        },
        "composer-attribute-collector": {
            "exclude": [
                "app/Support/PhpCsFixer/",
                "{vendor}/symplify/rule-doc-generator/"
            ],
            "include": [
                "app/"
            ]
        },
        "hooks": {
            "post-merge": [
                "composer checks"
            ],
            "pre-commit": [
                "composer checks"
            ]
        },
        "laravel": {
            "dont-discover": [
                "amirami/localizator",
                "andreaselia/laravel-api-to-postman",
                "barryvdh/laravel-ide-helper",
                "beyondcode/laravel-query-detector",
                "binarytorch/larecipe",
                "dedoc/scramble",
                "dragon-code/migrate-db",
                "dragon-code/pretty-routes",
                "guanguans/laravel-soar",
                "jasonmccreary/laravel-test-assertions",
                "josezenem/laravel-make-migration-pivot",
                "kitloong/laravel-migrations-generator",
                "knuckleswtf/scribe",
                "laracraft-tech/laravel-schema-rules",
                "laravel-shift/factory-generator",
                "laravel/dusk",
                "laravel/pail",
                "laravel/roster",
                "laravel/sail",
                "laravel/telescope",
                "msamgan/laravel-env-keys-checker",
                "muhammadhuzaifa/telescope-guzzle-watcher",
                "nunomaduro/collision",
                "orangehill/iseed",
                "rakutentech/laravel-request-docs",
                "reliese/laravel",
                "scalar/laravel",
                "soloterm/dumps",
                "soloterm/solo",
                "spatie/laravel-error-solutions",
                "spatie/laravel-horizon-watcher",
                "spatie/laravel-ignition",
                "spatie/laravel-login-link",
                "spatie/laravel-stubs",
                "sti3bas/laravel-scout-array-driver",
                "thedoctor0/laravel-factory-generator",
                "vcian/laravel-db-auditor",
                "worksome/envy",
                "worksome/request-factories"
            ]
        },
        "patches": {
            "laravel/framework": [
                "patches/laravel-framework-src-illuminate-foundation-application-php.patch"
            ],
            "laravel/pint": [
                "patches/laravel-pint-overrides-runner-parallel-processfactory-php.patch"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@cghooks add --ignore-lock"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --force --ansi -vv",
            "@artisan vendor:publish --tag=laravel-errors",
            "@artisan log-viewer:publish",
            "@artisan spatie-stub:publish --force",
            "@cghooks update",
            "@composer-diff"
        ],
        "pre-autoload-dump": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi -vv"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi -vv",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi -vv"
        ],
        "actionlint": "actionlint -color -oneline -verbose",
        "ai-commit": "@php ./vendor/bin/ai-commit commit --ansi",
        "ai-commit-bito": "@ai-commit --generator=bito_cli",
        "ai-commit-bito-no-verify": "@ai-commit-bito --no-verify",
        "ai-commit-github-copilot": "@ai-commit --generator=github_copilot_cli",
        "ai-commit-github-copilot-no-verify": "@ai-commit-github-copilot --no-verify",
        "ai-commit-github-models": "@ai-commit --generator=github_models_cli",
        "ai-commit-github-models-no-verify": "@ai-commit-github-models --no-verify",
        "ai-commit-no-verify": "@ai-commit --no-verify",
        "artisan": "@php artisan --ansi -vv",
        "artisan-envy-prune": "@artisan envy:prune",
        "artisan-envy-sync": "@artisan envy:sync",
        "artisan-ide-helper": [
            "@artisan ide-helper:eloquent",
            "@artisan ide-helper:generate",
            "@artisan ide-helper:meta",
            "@artisan ide-helper:models --nowrite"
        ],
        "artisan-solo": "@artisan solo",
        "artisan-solo-dumps": "@artisan solo:dumps",
        "artisan-xdebug": [
            "@env-put-xdebug-on",
            "@php artisan --ansi"
        ],
        "benchmark": "@php ./vendor/bin/phpbench run --report=aggregate --ansi -vv",
        "blade-formatter": "blade-formatter resources/views/*.blade.php resources/views/**/*.blade.php --ignore-path= --php-version=8.3 --progress",
        "blade-formatter-check-formatted": "@blade-formatter --check-formatted",
        "blade-formatter-write": "@blade-formatter --write",
        "cghooks": "@php ./vendor/bin/cghooks --ansi -vv",
        "cghooks-ignore": "[ ! -f \"./vendor/bin/cghooks\" ] && exit 0 || php ./vendor/bin/cghooks --ansi -vv",
        "checks": [
            "@env-put-xdebug-off",
            "@composer-normalize",
            "@composer-validate",
            "@composer-dependency-analyser",
            "@json-lint",
            "@md-lint",
            "@yaml-lint",
            "@phpmnd",
            "@style-tools-lint",
            "@style-lint",
            "@pint-bail",
            "@rule-doc-rector",
            "@facade-lint",
            "@sk-check-conflicts",
            "@sk-finalize-classes-dry-run",
            "@env-put-xdebug-on",
            "@pest-bail",
            "@env-put-xdebug-off",
            "@rector-dry-run",
            "@phpstan"
        ],
        "checks-verbose": "time /opt/homebrew/bin/composer checks",
        "class-leak": "@php ./vendor/bin/class-leak --ansi -vv",
        "class-leak-check": "@class-leak check app/ config/ --skip-suffix=Pipe --skip-path=Support/Traits --skip-path=Channels --skip-type=\\Guanguans\\LaravelExceptionNotify\\Contracts\\Channel --skip-type=\\Guanguans\\LaravelExceptionNotify\\Contracts\\Collector --skip-type=Rector\\Rector\\AbstractRector",
        "composer-bin-all-update": "@composer bin all update --ansi -vv",
        "composer-check-platform-reqs": "@composer check-platform-reqs --lock --ansi -vv",
        "composer-dependency-analyser": "@php ./vendor/bin/composer-dependency-analyser --verbose",
        "composer-diff": "@composer diff --with-platform --ansi -vv",
        "composer-disable-process-timeout": "Composer\\Config::disableProcessTimeout",
        "composer-normalize": "@composer normalize --diff --ansi -vv",
        "composer-normalize-dry-run": "@composer-normalize --dry-run",
        "composer-updater": [
            "Composer\\Config::disableProcessTimeout",
