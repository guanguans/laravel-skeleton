        "balping/json-raw-encoder": "^1.0",
        "bensampo/laravel-enum": "^6.11",
        "beyondcode/laravel-websockets": "^1.14",
        "biiiiiigmonster/hasin": "^3.1 || ^4.0",
        "bilfeldt/laravel-http-client-logger": "^2.2",
        "bramus/monolog-colored-line-formatter": "^3.1",
        "calebporzio/sushi": "^2.5",
        "cerbero/command-validator": "^2.5",
        "coduo/php-humanizer": "^5.0",
        "divineomega/password_exposed": "^3.2",
        "dragon-code/extended-routes": "^4.0",
        "dragon-code/laravel-http-logger": "^1.4",
        "dragon-code/laravel-route-names": "^1.5",
        "elao/enum": "^2.4",
        "elasticsearch/elasticsearch": "^8.13",
        "emreyarligan/enum-concern": "^1.0",
        "envant/fireable": "^1.0",
        "f9webltd/laravel-api-response-helpers": "^2.0",
        "gmponos/guzzle_logger": "^2.2",
        "guanguans/laravel-dump-sql": "^2.5",
        "guanguans/laravel-exception-notify": "dev-main",
        "guanguans/notify": "^2.6",
        "guzzlehttp/guzzle": "^7.8",
        "hamidrezaniazi/pecs": "^1.0",
        "hammerstone/fast-paginate": "^1.0",
        "hammerstone/flaky": "^0.1",
        "hammerstone/laravel-pseudo-daemon": "^1.1",
        "hashids/hashids": "^5.0",
        "jasny/sso": "^0.5",
        "jdavidbakr/laravel-cache-garbage-collector": "^2.2",
        "jenssegers/agent": "^2.6",
        "jiannei/laravel-response": "^6.0",
        "joetannenbaum/terminalia": "^0.3",
        "kevinrob/guzzle-cache-middleware": "^5.1",
        "knuckleswtf/scribe": "^4.31",
        "laravel-notification-channels/pushbullet": "^1.5",
        "laravel/framework": "^10.48 || ^11.0",
        "laravel/octane": "^2.3",
        "laravel/pennant": "^1.7",
        "laravel/pulse": "^1.0@beta",
        "laravel/sanctum": "^3.3 || ^4.0",
        "laravel/tinker": "^2.9",
        "neutrino_api/neutrino_api_client": "^4.6",
        "not-empty/jwt-manager-php-lib": "^6.0 || ^7.0",
        "nunomaduro/laravel-console-task": "^1.9",
        "nunomaduro/termwind": "^1.15 || ^2.0",
        "nyholm/psr7": "^1.8",
        "onlime/laravel-http-client-global-logger": "^1.1",
        "opcodesio/log-viewer": "^3.8",
        "orchid/platform": "^14.22",
        "overtrue/laravel-query-logger": "^3.2",
        "overtrue/laravel-uploader": "^3.2",
        "php-collective/decimal-object": "^1.2",
        "php-open-source-saver/jwt-auth": "^2.2",
        "php-static-analysis/attributes": "^0.2",
        "phpyh/lru-memoizer": "^0.2",
        "predis/predis": "^2.2",
        "protoqol/prequel": "^1.23",
        "pusher/pusher-php-server": "^7.2",
        "rahul900day/laravel-console-spinner": "^1.1",
        "rakutentech/laravel-request-docs": "^2.35",
        "rennokki/laravel-eloquent-query-cache": "^3.4",
        "revangelista/laravel-idempotency": "^1.0",
        "richardstyles/eloquent-aes": "^2.3",
        "sbamtr/laravel-query-enrich": "^1.2",
        "skywarth/chaotic-schedule": "^1.0",
        "spatie/fork": "^1.2",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-backup": "^8.6",
        "spatie/laravel-failed-job-monitor": "^4.3",
        "spatie/laravel-health": "^1.28",
        "spatie/laravel-medialibrary": "^11.4",
        "spatie/laravel-query-builder": "^5.8",
        "spatie/laravel-responsecache": "^7.5",
        "spatie/laravel-route-attributes": "^1.22",
        "spatie/laravel-route-discovery": "^1.0",
        "spatie/laravel-schedule-monitor": "^3.7",
        "spatie/laravel-schemaless-attributes": "^2.5",
        "spatie/laravel-settings": "^3.3",
        "spatie/laravel-short-schedule": "^1.6",
        "spatie/php-structure-discoverer": "^2.1",
        "sqids/sqids": "^0.4",
        "square1/laravel-idempotency": "^2.0",
        "staudenmeir/laravel-adjacency-list": "^1.20",
        "stillat/blade-directives": "^1.2",
        "symfony/polyfill-php83": "^1.29",
        "symfony/stopwatch": "^7.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^14.0",
        "tenantcloud/laravel-mixins": "^3.0",
        "timacdonald/has-parameters": "^1.4",
        "unicodeveloper/laravel-password": "^1.0",
        "valorin/pwned-validator": "^1.3",
        "vectorface/whip": "^0.5",
        "watson/validating": "^8.2",
        "wendelladriel/laravel-validated-dto": "^3.5",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "bennett-treptow/laravel-migration-generator": "^4.4",
        "brainmaestro/composer-git-hooks": "^3.0@alpha",
        "buggregator/trap": "^1.5",
        "coduo/php-matcher": "^6.0",
        "dedoc/scramble": "^0.8",
        "deployer/deployer": "^7.3",
        "dragon-code/benchmark": "^3.0",
        "driftingly/rector-laravel": "^1.1",
        "ergebnis/composer-normalize": "^2.42",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.53",
        "guanguans/ai-commit": "dev-main",
        "guanguans/laravel-soar": "^3.14",
        "kitloong/laravel-migrations-generator": "^7.0",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.21",
        "lanin/laravel-api-debugger": "^4.0",
        "laracademy/generators": "^3.7",
        "laracasts/generators": "^2.0",
        "laracraft-tech/laravel-schema-rules": "^1.3",
        "laradumps/laradumps": "^3.0",
        "larastan/larastan": "^2.9",
        "laravel-lang/common": "^6.2",
        "laravel-shift/factory-generator": "dev-master",
        "laravel/envoy": "^2.9",
        "laravel/facade-documenter": "dev-main",
        "laravel/pint": "^1.15",
        "laravel/sail": "^1.29",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.6",
        "mortexa/laravel-arkitect": "^0.7",
        "nunomaduro/collision": "^7.10 || ^8.0",
        "orangehill/iseed": "^3.0",
        "php-static-analysis/rector-rule": "^0.2",
        "phpstan/extension-installer": "^1.3",
        "phpunit/phpunit": "^10.5 || ^11.0",
        "plannr/laravel-fast-refresh-database": "^1.2",
        "rector/rector": "^1.0",
        "rector/swiss-knife": "^0.2",
        "reliese/laravel": "^1.3",
        "roave/security-advisories": "dev-latest",
        "salahhusa9/laravel-updater": "^1.2",
        "spatie/laravel-ignition": "^2.5",
        "thedoctor0/laravel-factory-generator": "^1.3",
        "tightenco/tlint": "^9.3",
        "worksome/envy": "^1.2"
    },
    "repositories": {
        "laravel-exception-notify": {
            "type": "path",
            "url": "./../../develop/laravel-exception-notify"
        },
        "facade-documenter": {
            "type": "vcs",
            "url": "https://github.com/laravel/facade-documenter.git"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true,
            "wikimedia/composer-merge-plugin": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": true,
            "target-directory": "vendor-bin"
        },
        "hooks": {
            "post-merge": [
                "composer checks"
            ],
            "pre-commit": [
                "composer checks"
            ]
        },
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper",
                "lanin/laravel-api-debugger",
                "nunomaduro/collision"
            ]
        },
        "merge-plugin": {
            "ignore-duplicates": false,
            "include": [
                "composer.local.json",
                "modules/*/composer.json"
            ],
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-replace": true,
            "merge-scripts": false,
            "recurse": true,
            "replace": false,
            "require": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@cghooks add --ignore-lock",
            "@cghooks update"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --force --ansi -v",
            "@php artisan ide-helper:generate --ansi -v",
            "@php artisan ide-helper:meta --ansi -v",
            "@cghooks update"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi -v"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi -v"
        ],
        "ai-commit": "@php ./vendor/bin/ai-commit commit --generator=bito_cli --ansi",
        "ai-commit-no-verify": "@ai-commit --commit-options=--no-verify",
        "arkitect": "@php artisan test:arkitect --ansi -v",
        "cghooks": "@php ./vendor/bin/cghooks --ansi -v",
        "cghooks-ignore": "[ ! -f \"./vendor/bin/cghooks\" ] && exit 0 || php ./vendor/bin/cghooks --ansi -v",
        "checks": [
            "@composer-validate",
            "@sk-check-conflicts",
            "@pint-dirty-test",
            "@phpstan",
            "@rector-dry-run"
        ],
        "composer-bin-all-update": "@composer bin all update --ansi -v",
        "composer-check-platform-reqs": "@composer check-platform-reqs --ansi -v",
        "composer-normalize": "@composer normalize --dry-run --diff --ansi -v",
        "composer-parallel": "@composer parallel --ansi -v",
        "composer-parallel-checks": "@composer-parallel composer-validate md-lint lint style-lint test psalm",
        "composer-require-checker": "@php ./vendor/bin/composer-require-checker check --config-file=composer-require-checker.json composer.json --ansi -v",
        "composer-unused": "@php ./vendor/bin/composer-unused --ansi -v",
        "composer-unused-checker": "@php ./vendor/bin/composer-unused --ansi -v",
        "composer-updater": "@php ./composer-updater --highest-php-binary=/opt/homebrew/opt/php@8.3/bin/php --ansi",
        "composer-updater-dry-run": "@composer-updater --dry-run",
        "composer-validate": "@composer validate --check-lock --strict --ansi -v",
        "docs-generate": "@php artisan scribe:generate --ansi -v",
        "envoy": "@php ./vendor/bin/envoy --ansi -v",
        "envoy-local": "@envoy run local",
        "envoy-ssh-testing": "@envoy ssh testing",
        "envoy-testing": "@envoy run testing",
        "envy-prune": "@php artisan envy:prune --ansi -v",
        "envy-sync": "@php artisan envy:sync --ansi -v",
        "facade-lint": "@facade-update --lint",
        "facade-update": "@php -f ./facade.php -- App\\\\Support\\\\Facades\\\\Elasticsearch App\\\\Support\\\\Facades\\\\OpenAI App\\\\Support\\\\Facades\\\\PushDeer",
        "ide-helper": [
            "@php artisan ide-helper:generate --ansi -v",
            "@php artisan ide-helper:models --nowrite --ansi -v",
            "@php artisan ide-helper:meta --ansi -v",
            "@php artisan admin:ide-helper --ansi -v"
        ],
        "lint": [
            "for DIR in .; do find $DIR -maxdepth 1 -type f -name '*.php' -type f ! -name 'xxx.php' -exec /opt/homebrew/opt/php@8.1/bin/php -l {} \\; 2>&1 | (! grep -v '^No syntax errors detected'); done",
            "for DIR in ./app ./config ./database ./lang ./resources ./routes ./tests; do find $DIR -type f -name '*.php' -type f ! -name 'xxx.php' -exec /opt/homebrew/opt/php@8.1/bin/php -l {} \\; 2>&1 | (! grep -v '^No syntax errors detected'); done"
        ],
        "md-fix": "@md-lint --fix",
        "md-lint": "lint-md --config .lintmdrc ./*.md ./.github/",
        "pest": "@php ./vendor/bin/pest --coverage",
        "pest-coverage": "@pest --coverage-html=./build/phpunit/ --coverage-clover=./clover.xml --coverage",
        "pest-migrate-configuration": "@pest --migrate-configuration",
        "phpstan": "@php ./vendor/bin/phpstan analyse --memory-limit=1G --ansi -v",
        "phpstan-baseline": "@phpstan --generate-baseline=phpstan-baseline.neon",
        "pint": "@php ./vendor/bin/pint --ansi -v",
        "pint-dirty": "@pint --dirty",
        "pint-dirty-test": "@pint-dirty --test --bail",
        "pint-test": "@pint --test",
        "rector": "@php ./vendor/bin/rector --ansi -v",
        "rector-clear-cache": "@rector --clear-cache",
        "rector-clear-cache-dry-run": "@rector-clear-cache --dry-run",
        "rector-custom-rule": "@rector custom-rule",
        "rector-detect-node": "@rector detect-node --loop",
        "rector-dry-run": "@rector --dry-run",
        "rector-list-rules": "@rector list-rules",
        "rector-setup-ci": "@rector setup-ci",
        "sk": "@php ./vendor/bin/swiss-knife --ansi -v",
        "sk-check-commented-code": "@sk check-commented-code ./app ./routes --line-limit=5",
        "sk-check-conflicts": "@sk check-conflicts ./app ./config ./database ./resources ./routes ./tests",
        "sk-finalize-classes": "@sk finalize-classes ./app",
        "sk-finalize-classes-dry-run": "@sk-finalize-classes --dry-run",
        "sk-find-multi-classes": "@sk find-multi-classes ./app ./routes",
        "sk-namespace-to-psr-4": "@sk namespace-to-psr-4 ./app --namespace-root=App\\",
        "style-fix": "@php ./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --diff --ansi -v",
        "style-lint": "@style-fix --dry-run",
        "test": "@pest",
        "test-coverage": "@pest-coverage",
        "test-migrate-configuration": "@pest-migrate-configuration",
        "tlint": "@php ./vendor/bin/tlint lint --diff --ansi -v",
        "tlint-format": "@php ./vendor/bin/tlint format --diff --ansi -v",
        "trap": "@php ./vendor/bin/trap --ui --ansi -v",
        "update-fluent-assert-doc-comment": "App\\Support\\FluentAssert::updateDocComment",
        "var-dump-server": "@php ./vendor/bin/var-dump-server --ansi -v"
    }
}
