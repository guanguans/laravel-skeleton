{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "bensampo/laravel-enum": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guanguans/laravel-dump-sql": "^1.2",
        "guanguans/laravel-exception-notify": "^1.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/agent": "^2.6",
        "jiannei/laravel-enum": "^1.4",
        "jiannei/laravel-response": "^1.8",
        "laravel/framework": "^8.54",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "overtrue/laravel-lang": "^5.0",
        "overtrue/laravel-query-logger": "^2.1",
        "spatie/laravel-json-api-paginate": "^1.10",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "beyondcode/laravel-query-detector": "^1.5",
        "brainmaestro/composer-git-hooks": "^2.8",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^2.7",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "stechstudio/laravel-php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "cghooks update"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "cghooks add --ignore-lock",
            "cghooks update"
        ],
        "cghooks": "./vendor/bin/cghooks",
        "check-style": "@php artisan fixer:fix --diff --dry-run --ansi",
        "fix-style": "@php artisan fixer:fix --ansi",
        "docs-generate": "@php artisan scribe:generate --ansi"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper",
                "knuckleswtf/scribe",
                "nunomaduro/collision"
            ]
        },
        "hooks": {
            "pre-commit": [
                "composer docs-generate",
                "composer check-style"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
